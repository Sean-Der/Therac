

<div data-context="{{ name }}">
  <h4 class="context-header"> {{name}} </h4>
  {{#each values}}
    <div class="context-name" data-context-name="{{this.name}}">
      <i class="fa fa-angle-up"></i>
      <span> {{this.name}} </span>
      <div class="context-value">{{this.value}}</div>
    </div>
  {{/each}}
</table>
